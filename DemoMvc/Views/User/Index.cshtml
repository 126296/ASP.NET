
@model List<User>

<h2>Student Data</h2>
<table class="table table-striped">
    <thead>
        <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Gender</th>
        <th>City</th>
        <th></th>
            </tr>
    </thead>
    <tbody>
        @for(int i=0 ; i<Model.Count; i++)
        {
        <tr>
            <td>@Model[i].id</td>
            <td>@Model[i].name</td>
            <td>@Model[i].gender</td>
            <td>@Model[i].city</td>
            <td></td>
        </tr>
        }
        
    </tbody>
    
</table>


